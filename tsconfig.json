{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "target": "es2015"
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/dashboard/form-mapping/components/DraggableText.jsx", "config/application.config.development.js", "config/application.config.js", "components/flow-chart/radio-node/RadioNode.jsx", "components/flow-chart/multi-select-node/MultiSelectNode.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
